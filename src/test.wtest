import remiseras.*
test "cuanto cobran" {
	anaMaria.indicarQueEstaEstable()
	assert.equals( roxana.cuantoCobra(ludmila, 10) == 180 )
	assert.equals( roxana.cuantoCobra(anaMaria, 10) == 300 )
